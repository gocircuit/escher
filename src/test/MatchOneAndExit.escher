MatchOneAndExit {
	*e.Ignore = `MatchOneAndExit matches the singular values incoming 
	on :Expected and :Got and then exits successfully (with exit code 0).`

	match *test.Match
	match:Expected = :Expected
	match:Got = :Got
	match: = door:Door

	door *e.OneWayDoor
	door:From = 0
	door:To = *os.Exit
}

